        function validateRegistrationDates(s, e) {
            var date1 = r1.GetDate();
            var date2 = r2.GetDate();

            if (date1 == null && date2 != null) {
                e.IsValid = false;
                s.innerText = "Please enter a valid FROM date.";
            }
            else if (date1 != null && date2 != null && date1 > date2) {
                e.IsValid = false;
                s.innerText = "The FROM date cannot be greater than the TO date.";
            }
            else
                e.IsValid = true;
        }

        function validateTrialDates(s, e) {
            var date1 = t1.GetDate();
            var date2 = t2.GetDate();

            if (date1 == null && date2 != null) {
                e.IsValid = false;
                s.innerText = "Please enter a valid FROM date.";
            }
            else if (date1 != null && date2 != null && date1 > date2) {
