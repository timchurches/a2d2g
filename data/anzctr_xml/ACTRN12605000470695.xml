  <conditions>
    <healthcondition>Cystic fibrosis</healthcondition>
    <conditioncode>
      <conditioncode1>Human genetics and inherited disorders</conditioncode1>
      <conditioncode2>Cystic fibrosis</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>The active intervention is non-invasive bi-level ventilatory support.</interventions>
    <comparator>The control intervention is spontaneous breathing on room air.  Each is applied for 90 minutes.</comparator>
    <control>Placebo</control>
    <interventioncode>None</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Whole right lung mucociliary clearance during a 90 minute treatment</outcome>
      <timepoint />
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Regional (central and peripheral) clearance.</outcome>
      <timepoint>During a 90 minute treatment.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Respiratory rate</outcome>
      <timepoint>During a 90 minute treatment.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
