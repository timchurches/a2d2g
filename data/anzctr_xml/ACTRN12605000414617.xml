    <utrn />
    <trialacronym />
  </trial_identification>
  <conditions>
    <healthcondition>Frailty in older people</healthcondition>
    <conditioncode>
      <conditioncode1>Physical Medicine / Rehabilitation</conditioncode1>
      <conditioncode2>Other physical medicine / rehabilitation</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Two weeks of weight-bearing resistance training is compared with two weeks of non weight-bearing resistance training.</interventions>
    <comparator />
    <control>Active</control>
    <interventioncode>Rehabilitation</interventioncode>
  </interventions>
