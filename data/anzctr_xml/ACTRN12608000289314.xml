<ANZCTR_Trial requestNumber="82868">
  <stage>Registered</stage>
  <submitdate>2/06/2008</submitdate>
  <approvaldate>10/06/2008</approvaldate>
  <actrnumber>ACTRN12608000289314</actrnumber>
  <trial_identification>
    <studytitle>The acute effects of resistance exercise on insulin sensitvity.</studytitle>
    <scientifictitle>Insulin sensitivity in response to an acute bout of resistance exercise in type 2 diabetic and non-diabetic individuals</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>Exercise and insulin sensitivity</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Type 2 diabetes and non-diabetic individuals</healthcondition>
    <conditioncode>
      <conditioncode1>Metabolic and Endocrine</conditioncode1>
      <conditioncode2>Diabetes</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Single session of resistance training using 5 exercises (bench press, leg press, shoulder press, calf raises, lat pull-down) completing 3 sets of 10 repetitions at 45%, 60% and 75% of one repetition maximum. This exercise session with cool down stretches takes approximately one hour to complete</interventions>
    <comparator>Non diabetic individuals who recieve the same single session of resistance exercise and type 2 diabetic individuals who complete no exercise</comparator>
    <control>Active</control>
    <interventioncode>Treatment: Other</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Insulin Sensitivity (Oral glucose tolerance test area under insulin curve) and Homeostasis model assessments (HOMA) calculations measured from blood samples</outcome>
      <timepoint>24 hours
48 hours
72 hours
96 hours</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Glucose tolerance (area under glucose curve) measured from blood samples</outcome>
      <timepoint>24 hours
48 hours
72 hours
96 hours</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>C-reactive protein measured from a fasting blood sample</outcome>
      <timepoint>24 hours
48 hours
72 hours
96 hours</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Type 2 diabetes
Sedentary (less than 20 minutes of aerobic exercise twice a week)</inclusivecriteria>
    <inclusiveminage>40</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>69</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>Yes</healthyvolunteer>
    <exclusivecriteria>Recent coronary event
Uncontrolled hypertension
Significant orthopaedic disorder that would prevent them from completing resistance training
Chronic alcohol users
Any other medical condition that has resistance training as a contraindication
Taking insulin
Inability to understand English or follow instructions</exclusivecriteria>
  </eligibility>
