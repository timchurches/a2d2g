  <trial_identification>
    <studytitle>Twin Birth Study</studytitle>
    <scientifictitle>The Twin birth study: A multicentre randomised controlled trial comparing planned vaginal birth to planned Caesarean Section of twins more than or equal to 32 weeks gestation to assess neonatal mortality and/or serious neonatal morbidity during the first 28 days after birth.</scientifictitle>
    <utrn />
    <trialacronym>TBS</trialacronym>
    <secondaryid>Perinatal Trials Registry: PTR540</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Twin pregnancy where twin A is cephalic and the gestational age is between 32-38 weeks.</healthcondition>
    <conditioncode>
      <conditioncode1>Reproductive Health and Childbirth</conditioncode1>
      <conditioncode2>Normal pregnancy</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Reproductive Health and Childbirth</conditioncode1>
      <conditioncode2>Complications of newborn</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Planned vaginal birth vs caesarean section planned</interventions>
    <comparator />
    <control>Active</control>
    <interventioncode>Other interventions</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Perinatal/neonatal mortality and/or serious neonatal morbidity</outcome>
      <timepoint>Within the first 28 days after delivery</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Death or poor neurodevelopemental outcome for children.</outcome>
      <timepoint>Collected at 2 years.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Twins where twin A is cephalic, gestational age 32-38 weeks, estimated fetal weight 1500-4000g, both twins alive.</inclusivecriteria>
    <inclusiveminage>32</inclusiveminage>
    <inclusiveminagetype>Weeks</inclusiveminagetype>
    <inclusivemaxage>38</inclusivemaxage>
    <inclusivemaxagetype>Weeks</inclusivemaxagetype>
    <inclusivegender>Females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>monoamniotic twins, lethal fetal anomaly, contraindication to labour or vaginal birth.</exclusivecriteria>
  </eligibility>
  <trial_design>
