<ANZCTR_Trial requestNumber="448">
  <stage>Registered</stage>
  <submitdate>7/09/2005</submitdate>
  <approvaldate>9/09/2005</approvaldate>
  <actrnumber>ACTRN12605000357651</actrnumber>
  <trial_identification>
    <studytitle>SNAC Trial</studytitle>
    <scientifictitle>A randomised phase III study to determine in women with early breast cancer whether sentinel node biopsy and axillary clearance if positive, results in less morbidity than immediate axillary clearance with equivalent cancer-related outcomes.</scientifictitle>
    <utrn />
    <trialacronym>SNAC Trial</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Operable breast cancer</healthcondition>
    <conditioncode>
      <conditioncode1>Cancer</conditioncode1>
      <conditioncode2>Breast</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Sentinel node biopsy with immediate standard axillary clearance vs sentinel node biopsy alone with axillary clearance if sentinel node is not identified or sentinel node is positive for cancer.</interventions>
    <comparator />
    <control>Active</control>
    <interventioncode>Treatment: surgery</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Early axillary morbidity</outcome>
      <timepoint>Measured at baseline, one month after definitive axillary surgery, 6 months after randomisation and annually for 5 years.</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Observer ratings of arm swelling, shoulder movement, sensation</outcome>
      <timepoint>Measured at baseline, one month after definitive axillary surgery, 6 months after randomisation and annually for 5 years.</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>QOL ratings</outcome>
      <timepoint>Measured at baseline, one month after definitive axillary surgery, 6 months after randomisation and annually for 5 years.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>1. Axillary recurrence rates</outcome>
      <timepoint>Measured 1 month  post definitive axillary surgery, 6 months after randomisation, and annually for at least 10 years.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>2. Relapse free survival and overall survival</outcome>
      <timepoint>Measured 1 month  post definitive axillary surgery, 6 months after randomisation, and annually for at least 10 years.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>3. Use of adjuvant radiation therapy, chemotherapy and endocrine therapy.</outcome>
      <timepoint>Measured  at 1 year post randomisation.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>4.Number of surgical episodes and days in hospital.</outcome>
      <timepoint>Measured 1 month post randomisation, 6 months post randomisation and yearly for 5 years.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Histologically or cytologically confirmed invasive breast cancer.</inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
    <inclusiveminagetype>Not stated</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>Not stated</inclusivemaxagetype>
    <inclusivegender>Females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>In situ cancer only. Clinically involved nodes where axillary surgery is deemed essential. Multicentric tumours. Primary tumour &gt;3cm in diameter. Evidence of metastatic disease.</exclusivecriteria>
  </eligibility>
