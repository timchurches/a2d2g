    <studytitle>Ketamine - Investigate the Dose Study</studytitle>
    <scientifictitle>Pharmacodynamics of Ketamine in Children during emergency medical procedures.</scientifictitle>
    <utrn />
    <trialacronym>KIDS</trialacronym>
    <secondaryid>Auckland District Health Board: A+3166</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Pain, sedation and distress during emergency medical procedures.</healthcondition>
    <conditioncode>
      <conditioncode1>Other</conditioncode1>
      <conditioncode2>Conditions of unknown or disputed aetiology (such as chronic fatigue syndrome/myalgic encephalomyelitis)</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Pharmacodynamic study of Ketamine 1mg/kg</interventions>
    <comparator>Pain, sedation and distress.</comparator>
    <control />
    <interventioncode>Treatment: drugs</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Ketamine concentration at peak, during the procedure and after the procedure.</outcome>
      <timepoint>Measured at peak (5 minutes), during the procedure (approximately 30 minutes) and after the procedure.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Behavioural Measures of Distress - Wisconsin Sedation Scale</outcome>
      <timepoint>Measured at same time as Ketamine levels.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Visual Analog Scale</outcome>
      <timepoint>Measured at same time as Ketamine levels.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Observer Scale of Behavioural Distress.</outcome>
      <timepoint>Measured at same time as Ketamine levels.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>All children suitable for Ketamine sedation as per Auckland Starship Childrens hospital guidelines.</inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
