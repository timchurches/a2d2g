  <actrnumber>ACTRN12607000127404</actrnumber>
  <trial_identification>
    <studytitle>Hypnosis for nausea and vomiting in early pregnancy: a randomised controlled trial</studytitle>
    <scientifictitle>The effects of antenatal hypnosis on suffering associated with hyperemesis in early pregnancy</scientifictitle>
    <utrn />
    <trialacronym />
  </trial_identification>
  <conditions>
    <healthcondition>Hyperemesis in early pregnancy</healthcondition>
    <conditioncode>
      <conditioncode1>Reproductive Health and Childbirth</conditioncode1>
      <conditioncode2>Antenatal care</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Intervention: Usual Care (supportive and pharmacological medication and /or intravenous fluids as required) plus Audio CD on Hypnosis lasting 1/2 hour for 7 consecutive days</interventions>
    <comparator>Control: Usual Care (as described above)</comparator>
    <control>Active</control>
    <interventioncode>Treatment: Other</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Suffering associated with nausea and vomiting as measured by 5 point Likert Scale</outcome>
      <timepoint>On days 2, 7 and 14 following allocation</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Incidence of Nausea in previous 24 hours </outcome>
      <timepoint>On Days 2,7,14</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Incidence of Vomiting in previous 24 hours </outcome>
      <timepoint>On Days 2,7,14</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Anxierty as measured by Spielberger </outcome>
      <timepoint>Baseline and at 14 days</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Number of Days off work at Day 14 in previous 7 days</outcome>
      <timepoint>Baseline and day 14</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Suffering from nausea and vomiting of pregnancy.</inclusivecriteria>
    <inclusiveminage>16</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>Not stated</inclusivemaxagetype>
    <inclusivegender>Females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Active psychological or psychiatric illness, poor understanding of english; underlying medical or obstetric condition contributing to nausea and vomiting.</exclusivecriteria>
  </eligibility>
