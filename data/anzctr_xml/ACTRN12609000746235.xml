<ANZCTR_Trial requestNumber="83685">
  <stage>Registered</stage>
  <submitdate>8/03/2009</submitdate>
  <approvaldate>27/08/2009</approvaldate>
  <actrnumber>ACTRN12609000746235</actrnumber>
  <trial_identification>
    <studytitle>In chronic tension type headache sufferers is sauna or soft tissue massage more effective than their current care in reducing headache severity and frequency.</studytitle>
    <scientifictitle>In chronic tension type headache sufferers is sauna or soft tissue massage more effective than their current care in reducing headache severity and frequency</scientifictitle>
    <utrn />
    <trialacronym>WEST Headache trial</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Chronic tension type headache</healthcondition>
    <conditioncode>
      <conditioncode1>Neurological</conditioncode1>
      <conditioncode2>Studies of the normal brain and nervous system</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Alternative and complementary medicine</conditioncode1>
      <conditioncode2>Other alternative and complementary medicine</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Sauna, Three times a week for 20 minutes for eight weeks.</interventions>
    <comparator>Soft tissue Massage. Five minutes per day, self administered in the head and neck muscles for eight weeks.</comparator>
    <control>Active</control>
    <interventioncode>Treatment: Other</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Time head pain present during day measured in hours. Diary is filled in daily.</outcome>
      <timepoint>Measured daily for four weeks prior to interventions and daily for two months after intervetion</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Pain severity measured by Visual Analogue Pain score out of 10.</outcome>
      <timepoint>Measured daily for four weeks prior to interventions and for two months after intervetion.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Depression as measured by the Beck Depression Inventory.</outcome>
      <timepoint>At four weeks prior to intervention and 8 weeks after intervention.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Chronic tension Type Headache as defined by the International headache society (IHS).</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>70</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Known cause of symptoms such as acute sinusitis, eyesight or ear problems, 
Current diagnosis of cancer 
Acute infections including viral illness
Previous spinal/brain surgery 
Pregnancy
Head injury</exclusivecriteria>
  </eligibility>
