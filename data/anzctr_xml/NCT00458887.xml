  <trial_identification>
    <studytitle>Assessing Ear Damage in Young Cancer Patients Treated With Cisplatin</studytitle>
    <scientifictitle>A Group-Wide, Prospective Study of Ototoxicity Assessment in Children Receiving Cisplatin Chemotherapy</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>COG-ACCL05C1</secondaryid>
