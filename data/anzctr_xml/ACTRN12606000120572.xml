      <timepoint>12 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Parenting</outcome>
      <timepoint>12 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Parental BMI</outcome>
      <timepoint>12 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Eating and activity behaviours</outcome>
      <timepoint>12 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Health belief</outcome>
      <timepoint>12 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Program satisfaction.</outcome>
      <timepoint>12 months</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Overweight, pre-pubertal, caregiver able to attend sessions and read and understand English.  </inclusivecriteria>
    <inclusiveminage>5</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>9</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>BMI Z score &gt; 4.0, syndromal obesity, medications influencing weight gain/loss, physical/developmental disability, chronic illness , significant dietary restriction, sibling enrolled.</exclusivecriteria>
  </eligibility>
