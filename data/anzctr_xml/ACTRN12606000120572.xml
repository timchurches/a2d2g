      <timepoint>12 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Health related quality of life</outcome>
      <timepoint>12 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
