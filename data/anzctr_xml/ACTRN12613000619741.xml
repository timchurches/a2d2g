  <trial_identification>
    <studytitle>An observational study of the incidence, prevalence and clinical outcomes of primary vasculitis in New Zealand</studytitle>
    <scientifictitle>Incidence, Prevalence, biomarkers and clinical outcomes of primary vasculitis</scientifictitle>
    <utrn />
    <trialacronym>NZVASC</trialacronym>
    <secondaryid>NIL</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Vasculitis</healthcondition>
    <conditioncode>
      <conditioncode1>Inflammatory and Immune System</conditioncode1>
      <conditioncode2>Autoimmune diseases</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Disease outcomes including death, internal organ involvement, and treatment side effects will be collected for up to 20 years</interventions>
    <comparator>Not applicable</comparator>
    <control>Uncontrolled</control>
    <interventioncode>Not applicable</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Incidence of vasulitis in NZ will be calculated as the number of  newly diagnosed patients within 1, 5 and 10 years.</outcome>
      <timepoint>1, 5 and 10 years</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Disease outcomes in vasculitis will be assessed using validated scores: Vasculitis Disease Index, Birmingham Vasculitis Activity Score, Disease Extent Index</outcome>
      <timepoint>5 years</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Treatments required for vasculitis in NZ will be assessed by names of specific drugs</outcome>
      <timepoint>up to 20 years</timepoint>
    </secondaryOutcome>
  </outcomes>
