  <trial_identification>
    <studytitle>Enzastaurin Versus Lomustine in Glioblastoma</studytitle>
    <scientifictitle>Randomized Phase 3 Open Label Study - Enzastaurin vs. Lomustine in Glioblastoma</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>H6Q-MC-JCBF</secondaryid>
