  <trial_identification>
    <studytitle>Power spectral analysis and inverse modeling of the electroencephalogram to monitor sleepiness on a driving simulator during sleep deprivation</studytitle>
    <scientifictitle>The effect of 40 hours sleep deprivation on sleepiness, driving simulator performance and electrographic power in healthy volunteers and patients with obstructive sleep apnea</scientifictitle>
    <utrn />
    <trialacronym />
  </trial_identification>
  <conditions>
    <healthcondition>Obstructive sleep apnea.  </healthcondition>
    <healthcondition>Healthy volunteers also studied.</healthcondition>
    <conditioncode>
      <conditioncode1>Respiratory</conditioncode1>
      <conditioncode2>Sleep apnoea</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Sleep deprivation 40 hours</interventions>
    <comparator>non-sleep deprived state (measured at baseline before 40-hour sleep deprivation period, time = zero hours)</comparator>
    <control>Active</control>
    <interventioncode>None</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Subjective sleepiness.
Note that the key aim is to model the progression of the outcomes across the 40 hour intervention period.</outcome>
      <timepoint>Measured every 2 hours during study.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Driving simulator performance</outcome>
      <timepoint>Measured every 2 hours.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Quantitative electroencephalography measures</outcome>
      <timepoint>Measured every 2 hours.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Healthy group: n = 12 without sleep disorder.OSA group: n = 12.</inclusivecriteria>
    <inclusiveminage>25</inclusiveminage>
