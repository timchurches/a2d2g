<ANZCTR_Trial requestNumber="83069">
  <stage>Registered</stage>
  <submitdate>13/08/2008</submitdate>
  <approvaldate>30/09/2008</approvaldate>
  <actrnumber>ACTRN12608000501347</actrnumber>
  <trial_identification>
    <studytitle>Prophylactic Antibiotics for spontaneous prelabor rupture of membranes at term</studytitle>
    <scientifictitle>Prophylactic antibiotics for spontaneous prelabor rupture of membranes at term to reduce early neonatal sepsis</scientifictitle>
    <utrn>U1111-1122-0540</utrn>
    <trialacronym>PAST PROM</trialacronym>
    <secondaryid>ASU2010</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>prelabor rupture of membranes at term</healthcondition>
    <conditioncode>
      <conditioncode1>Reproductive Health and Childbirth</conditioncode1>
      <conditioncode2>Childbirth and postnatal care</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>prophylactic antibiotics
ampicillin/flucloxacillin
500 mg intravenously every 6 hours
started on admission till delivery</interventions>
    <comparator>The solvent of the antibiotic vial every 6 hours from admission till delivery</comparator>
    <control>Placebo</control>
    <interventioncode>Prevention</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>early neonatal sepsis diagnosed clinically and by laboratory markers of sepsis</outcome>
      <timepoint>at 1 week after randomisaton</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>maternal postpartum febrile morbidity diagnosed clinically</outcome>
      <timepoint>at 2 weeks after randomisation</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>spontaneous rupture of fetal membranes at 36 weeks gestation or beyond</inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
    <inclusiveminagetype>No limit</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>No limit</inclusivemaxagetype>
    <inclusivegender>Females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>chorioamnionitis
prolonged rupture of membranes &gt;24</exclusivecriteria>
  </eligibility>
