      <outcome>Proportion of subjects with AEs leading to discontinuation</outcome>
      <timepoint>Day 1 to 56 days after the last dose of study drug</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Proportion of Deaths</outcome>
      <timepoint>Day 1 to 56 days after the last dose of study drug</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Proportion of subjects with laboratory abnormalities</outcome>
      <timepoint>Day 1 to 56 days after the last dose of study drug</timepoint>
    </secondaryOutcome>
