    <control>Uncontrolled</control>
    <interventioncode>Prevention</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Hepatitis B surface antibody measurement</outcome>
      <timepoint>At baseline and  2-4 weeks post vaccination</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Hepatitis B surface antigen</outcome>
      <timepoint>Measurement at baseline</timepoint>
    </secondaryOutcome>
  </outcomes>
