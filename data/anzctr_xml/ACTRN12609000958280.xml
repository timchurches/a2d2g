  <actrnumber>ACTRN12609000958280</actrnumber>
  <trial_identification>
    <studytitle>Cluster Hole Versus Solid Cup Fixation in Total Hip Arthroplasty: 
A Randomised Clinical Trial</studytitle>
    <scientifictitle>A study to compare cluster hole and solid acetabular cups for lysis at five years following total hip arthroplasty in patients with osteoarthritis or rheumatoid arthritis.</scientifictitle>
    <utrn />
    <trialacronym />
  </trial_identification>
  <conditions>
    <healthcondition>Osteoarthritis</healthcondition>
    <healthcondition>Rheumatoid arthritis</healthcondition>
    <conditioncode>
      <conditioncode1>Surgery</conditioncode1>
      <conditioncode2>Surgical techniques</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Patient will undergo total hip arthroplasty. The acetabular component will be a cluster hole cup. The operation will take 1-2 hours.</interventions>
    <comparator>Patient will undergo total hip arthroplasty. The acetabular component will be a solid cup. The operation will take 1-2 hours. .</comparator>
    <control>Active</control>
    <interventioncode>Treatment: surgery</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Osteolysis. This will be measured with a computed tomography (CT) scan.</outcome>
      <timepoint>5 years post-surgery</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Cup Migration. This will be measured with a computed tomography (CT) scan.</outcome>
      <timepoint>5 years post-surgery</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Complications. These will include failure of prosthesis, infection, bleeding. This will be assessed at patient follow-up.</outcome>
      <timepoint>5 years post-surgery</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Any patient requiring total hip arthroplasty for osteoarthritis or rheumatoid arthritis.</inclusivecriteria>
    <inclusiveminage>50</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>95</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Post-traumatic, dysplastic hip, shallow acetabulum.</exclusivecriteria>
  </eligibility>
