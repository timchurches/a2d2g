  <trial_identification>
    <studytitle>THE EFFECT OF PRETERM BIRTH ON OUTCOME - A RANDOMISED TRIAL OF PREVENTATIVE CARE</studytitle>
    <scientifictitle>A RANDOMISED TRIAL OF PREVENTATIVE CARE AT HOME IN VERY PRETERM INFANTS ON NEUROSENSORY OUTCOME AT 2 YEARS OF AGE</scientifictitle>
    <utrn />
    <trialacronym>Vibes plus</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>SURVIVING VERY PRETERM INFANTS(&lt;30 WEEKS' GESTATIONAL AGE)</healthcondition>
    <conditioncode>
      <conditioncode1>Reproductive Health and Childbirth</conditioncode1>
      <conditioncode2>Complications of newborn</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Developmental care administered in the home to surviving infants born &lt; 30 weeks' gestational age by a physiotherapist and a psychologist over the first year of life.</interventions>
    <comparator>standard follow-up to 2 years of age</comparator>
    <control>Active</control>
    <interventioncode>Treatment: Other</interventioncode>
  </interventions>
