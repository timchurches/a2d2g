  <trial_identification>
    <studytitle>Prospective Cohort Observational Study of Peripheral Inserted Central Catheters using Ultrasound</studytitle>
    <scientifictitle>Ultrasound reduces time for insertion of Peripheral inserted central catheters in patients requiring peripheral inserted central catheters at the discretion of their treating doctors by referal</scientifictitle>
    <utrn />
    <trialacronym>PICCUP</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Insertion of Peripheral Inserted Central Catheters</healthcondition>
    <conditioncode>
      <conditioncode1>Anaesthesiology</conditioncode1>
      <conditioncode2>Anaesthetics</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Use of ultrasound to facilitate Peripheral Inserted Central Catheter insertion by locating the vein to be cannulated and by facilitating needle insertion under direct ultrasonic vision</interventions>
    <comparator>Peripheral Inserted Central Catheters without ultrasound</comparator>
    <control>Active</control>
    <interventioncode>Treatment: Devices</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Time for insertion of peripheral inserted central catheter</outcome>
      <timepoint>At time of insertion</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Vein visibility to the naked eye</outcome>
      <timepoint>at time of insertion</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>number of attempts</outcome>
      <timepoint>at time of insertion</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>patient satisfaction with questionnaire</outcome>
      <timepoint>4 weeks post peripheral inserted central catheter insertion</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>PICC complications ie thrombosis of upper limb veins (clinical evaluation), occlusion of lumens of peripheral inserted central catheter, local or systemic infection (clinical evaluation)</outcome>
      <timepoint>4 weeks post peripheral inserted central catheter insertion</timepoint>
    </secondaryOutcome>
  </outcomes>
