  <actrnumber>ACTRN12611000717954</actrnumber>
  <trial_identification>
    <studytitle>Efficacy of clorpactin('registered trademark (R)) in bladder pain syndrome/interstitial cystitis: a randomised placebo-controlled trial</studytitle>
    <scientifictitle>Efficacy of clorpactin(R) in bladder pain syndrome/interstitial cystitis: a randomised placebo-controlled trial</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>Nil</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Bladder pain syndrome/interstitial cystitis</healthcondition>
    <conditioncode>
      <conditioncode1>Renal and Urogenital</conditioncode1>
      <conditioncode2>Other renal and urogenital disorders</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Clorpactin (sodium oxychlorosene) 0.4% solution (2gm in 500ml sterile water) given as a single intravesical instillation under general anaesthetic</interventions>
    <comparator>Placebo (cystodistension) - distension for 4 minutes having been filled to capacity under 80 cm of water pressure</comparator>
    <control>Placebo</control>
    <interventioncode>Treatment: drugs</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Subject improvement as reflected in the Global response assessment</outcome>
      <timepoint>3 months</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Nil</outcome>
      <timepoint>NIl</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Females 18-70 years old
Diagnosed with bladder pain syndrome/interstitial cystitis</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>70</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Allergy to clorpactin(R) or its constituent parts
History of renal impairment or vesico-ureteric reflux
Contraindication to general anaesthetic
Unable to sign consent
Unable to attend followup or complete questionnaires</exclusivecriteria>
  </eligibility>
