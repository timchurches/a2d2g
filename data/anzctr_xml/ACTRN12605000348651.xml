  <actrnumber>ACTRN12605000348651</actrnumber>
  <trial_identification>
    <studytitle>Effect of Positive Expiratory Pressure (PEP) Physiotherapy on the Duration of the Exacerbation-Free Period in People with Cystic Fibrosis (CF)</studytitle>
    <scientifictitle>Effect of Positive Expiratory Pressure (PEP) Physiotherapy on the Duration of the Exacerbation-Free Period in People with Cystic Fibrosis (CF)</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>X03-0028</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Cystic fibrosis</healthcondition>
    <conditioncode>
      <conditioncode1>Human genetics and inherited disorders</conditioncode1>
      <conditioncode2>Cystic fibrosis</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Physical Medicine / Rehabilitation</conditioncode1>
      <conditioncode2>Physiotherapy</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Positive expiratory pressure (PEP) physiotherapy (b.d.). For 24 weeks or until the first exacerbation.</interventions>
    <comparator>No physiotherapy</comparator>
    <control>Placebo</control>
    <interventioncode>None</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Duration of the Exacerbation-Free Period up to 24 weeks.</outcome>
      <timepoint />
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Lung function</outcome>
      <timepoint>For 24 weeks or until the first exacerbation.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Quality of life</outcome>
      <timepoint>For 24 weeks or until the first exacerbation.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Exercise tolerance</outcome>
      <timepoint>For 24 weeks or until the first exacerbation.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Medication use</outcome>
      <timepoint>For 24 weeks or until the first exacerbation.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Exercise tolerance</outcome>
      <timepoint>At 6 weeks</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Mucus plugging on high resolution CT</outcome>
      <timepoint>At 6 weeks</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Cystic fibrosis, stable clinical condition with an FEV1 within 15% of their best recorded value as an outpatient for the preceding six months and no non-routine antibiotic use within the preceding two weeks. </inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
    <inclusiveminagetype>Not stated</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>Not stated</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>FEV1 &lt;30%pred, PaO2 &lt;50mmHg, PaCO2 &gt;55mmHg, or are listed for transplant. Subjects will be required to have had at least one exacerbation requiring hospital admission for antibiotic therapy within the past year.</exclusivecriteria>
  </eligibility>
