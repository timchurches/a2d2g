  <trial_identification>
    <studytitle>A Study of rhBMP-2/CPM in Closed Fractures of the Humerus</studytitle>
    <scientifictitle>A Phase 2, Multicenter, Double-Blind, Randomized, Stratified, Controlled, Efficacy, Safety and Feasibility Study of Recombinant Human Bone Morphogenetic Protein-2 (rhBMP-2)/Calcium Phosphate Matrix (CPM) as an Adjuvant Therapy in Closed Fractures of the Humerus</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>3100N7-212</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Fractures</healthcondition>
    <conditioncode>
      <conditioncode1>Injuries and Accidents</conditioncode1>
      <conditioncode2>Fractures</conditioncode2>
    </conditioncode>
