  <outcomes>
    <primaryOutcome>
      <outcome>Proportion of patients with plasma HIV-1 RNA &lt;50 copies/mL at W24</outcome>
      <timepoint>week 24</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Time to loss of virological response (TLOVR analysis; FDA algorithm) at W12, W24 and W48 (&lt;50 copies/mL)</outcome>
      <timepoint>week 12, 24, and 48</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Proportion of patients with plasma HIV-1 RNA &lt;50 copies/mL at W48</outcome>
      <timepoint>week 48</timepoint>
    </secondaryOutcome>
  </outcomes>
