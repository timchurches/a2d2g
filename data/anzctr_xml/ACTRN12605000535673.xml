<ANZCTR_Trial requestNumber="798">
  <stage>Registered</stage>
  <submitdate>27/09/2005</submitdate>
  <approvaldate>28/09/2005</approvaldate>
  <actrnumber>ACTRN12605000535673</actrnumber>
  <trial_identification>
    <studytitle>The acute effects of physiotherapy on gas exchange and well-being in patients with cystic fibrosis.</studytitle>
    <scientifictitle>The acute effects of physiotherapy on gas exchange and well-being in patients with cystic fibrosis.</scientifictitle>
    <utrn />
    <trialacronym>Physiotherapy and gas exchange</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Cystic fibrosis.</healthcondition>
    <conditioncode>
      <conditioncode1>Human genetics and inherited disorders</conditioncode1>
      <conditioncode2>Cystic fibrosis</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Physical Medicine / Rehabilitation</conditioncode1>
      <conditioncode2>Physiotherapy</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Randomized crossover study looking at the acute effects of a single chest physiotherapy treatment on detailed gas exchange measured by MIGET (multiple inert gas elimination technique).</interventions>
    <comparator />
    <control>Placebo</control>
    <interventioncode>Treatment: Other</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>The effects of a chest physiotherapy treatment on spirometry</outcome>
      <timepoint>2 hours after therapy</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The effects of a chest physiotherapy treatment on  ABG</outcome>
      <timepoint>2 hours after therapy</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The effects of a chest physiotherapy treatment on MIGET gas exchange</outcome>
      <timepoint>2 hours after therapy</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The effects of a chest physiotherapy treatment on well-being</outcome>
      <timepoint>2 hours after therapy</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>The correlation between physiological changes following chest physiotherapy and well-being.</outcome>
      <timepoint>Two hours following physiotherapy therapy.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>A diagnosis of cystic fibrosis, stable state with the production of at least 30ml sputum with chest physiotherapy.</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>Not stated</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Factors compromising chest physiotherapy eg. pneumothorax.</exclusivecriteria>
  </eligibility>
