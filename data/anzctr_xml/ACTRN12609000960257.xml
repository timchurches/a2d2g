  <stage>Registered</stage>
  <submitdate>5/11/2009</submitdate>
  <approvaldate>6/11/2009</approvaldate>
  <actrnumber>ACTRN12609000960257</actrnumber>
  <trial_identification>
    <studytitle>Cruciate Retaining vs Posterior Stabilised Total Knee Replacement: A Randomised Clinical Trial</studytitle>
    <scientifictitle>A randomised trial to compare a cruciate retaining prosthesis with a posterior stabilised prosthesis for functional outcomes in total knee arthroplasty</scientifictitle>
    <utrn />
    <trialacronym />
  </trial_identification>
  <conditions>
    <healthcondition>Osteoarthritis</healthcondition>
    <healthcondition>Rheumatoid arthritis</healthcondition>
    <conditioncode>
      <conditioncode1>Inflammatory and Immune System</conditioncode1>
      <conditioncode2>Rheumatoid arthritis</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Musculoskeletal</conditioncode1>
      <conditioncode2>Osteoarthritis</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Patient will undergo total knee arthroplasty. The prosthesis used will be a posterior stabilised device. The operation will take 2 hours.</interventions>
    <comparator>Patient will undergo total knee arthoplasty. The prosthesis used will be a cruciate retaining device. The operation will take 2 hours.</comparator>
    <control>Active</control>
    <interventioncode>Treatment: surgery</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Oxford Knee Score (OKS). This assesses knee function.</outcome>
      <timepoint>2 weeks, 6 weeks, 3 months, 12 months, 2 years, 5 years and 10 years post-operatively</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Active Range of Motion in degrees measured with a goniometer</outcome>
      <timepoint>2 weeks, 6 weeks, 3 months, 12 months, 2 years, 5 years and 10 years post-operatively</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>6 minute walk test. This tests the distance a patient can walk in 6 minutes</outcome>
      <timepoint>2 weeks, 6 weeks, 3 months, 12 months, 2 years, 5 years and 10 years post-operatively</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Primary total knee replacement</inclusivecriteria>
    <inclusiveminage>40</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>90</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Patients will be excluded from the trial for tibial deformity (including past fracture or high tibial osteotomy) or valgus deformity.</exclusivecriteria>
  </eligibility>
