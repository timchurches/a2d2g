    <control />
    <interventioncode>Treatment: drugs</interventioncode>
    <interventioncode>Other interventions interventions</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Efficacy - International Prostate Symptom Score (IPSS) total score change from baseline over 52 weeks.</outcome>
      <timepoint>Week 52</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - Qmax change from baseline over 52 weeks.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - IPSS total score change from baseline at each individual post-baseline timepoint.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - Qmax change from baseline at each individual post-baseline timepoint.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - IPSS "responders" at each individual post-baseline timepoint.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - Qmax "responders" at each individual post-baseline timepoint.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - Proportion of patients who receive rescue therapy.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - Time to onset of rescue therapy.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - Incidence rate for episodes of urinary retention.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - Transition Zone (TZ) prostate volume change from baseline as measured by transrectal ultrasound (TRUS) at each individual post-baseline timepoint.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Efficacy - Total prostate volume (PV) change from baseline as measured by TRUS at each individual post-baseline timepoint.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Treatment-emergent adverse events (TEAEs).</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Episodes of acute urinary retention as determined by the independent Adjudication Panel.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Assessment of sexual function for men who are sexually active using the International Index of Erectile Function - Erectile Function (IIEF-EF) and the Male Sexual Health Questionnaire© short form for ejaculatory dysfunction (MSHQ-EjD).</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Physical examinations.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Vital signs.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Electrocardiograms (ECGs).</outcome>
      <timepoint>Week 6</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Laboratory parameters, consisting of chemistry panel, complete blood count (CBC), and urinalysis.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Measurement of anti-PRX302 antibodies (APA).</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Safety - Serum concentration of PRX302 only if clinically indicated by an event such as suspected systemic toxicity.</outcome>
      <timepoint>Week 52</timepoint>
    </secondaryOutcome>
