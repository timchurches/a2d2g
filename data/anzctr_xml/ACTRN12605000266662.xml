  <trial_identification>
    <studytitle>Metformin in Gestational Diabetes</studytitle>
    <scientifictitle>Gestational Diabetes: pregnancy outcomes in treatment with metformin compared with insulin.</scientifictitle>
    <utrn />
    <trialacronym>MiG</trialacronym>
    <secondaryid>Perinatal Trials Registry: PTR521</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Gestational diabetes</healthcondition>
    <conditioncode>
      <conditioncode1>Metabolic and Endocrine</conditioncode1>
      <conditioncode2>Diabetes</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Reproductive Health and Childbirth</conditioncode1>
      <conditioncode2>Fetal medicine and complications of pregnancy</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Women with gestational diabetes requiring treatment are randomised to insulin or metformin treatment for the duration of pregnancy.</interventions>
    <comparator />
    <control>Active</control>
    <interventioncode>Treatment: drugs</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Composite of neonatal morbidity including hypoglycaemia, respiratory distress, prematurity, phototherapy birth trauma, and low Apgar.</outcome>
      <timepoint>At 5 minutes</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Maternal glycaemia control</outcome>
      <timepoint />
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Neonatal anthropometry</outcome>
      <timepoint />
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Cord blood measures of adipoinsular axis</outcome>
      <timepoint />
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Maternal hypertensive complications and postpartum GTT results</outcome>
      <timepoint />
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Acceptability of treatments.</outcome>
      <timepoint />
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>22-33 weeks' gestation, singleton pregnancy, capillary glucose levels requiring additional treatment.</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>45</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Diabetes diagnosed pregnancy, gestational hypertension, pre-eclampsia of fetal growth restriction at study entry, fetal congenital anomaly, maternal medical conditon posing contra-indication to metformin.</exclusivecriteria>
  </eligibility>
