  <actrnumber>ACTRN12605000114640</actrnumber>
  <trial_identification>
    <studytitle>Hypofractionated radiotherapy for the palliation of patients with advanced head and neck cancer who are unsuitable for an attempt at curative treatment</studytitle>
    <scientifictitle>Assessment of quality of life of patients undergoing hypofractionated radiotherapy for the palliation of patients with advanced head and neck cancer who are unsuitable for an attempt at curative treatment</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>Hypofractionated head and neck study</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Advanced head and neck cancer</healthcondition>
    <conditioncode>
      <conditioncode1>Cancer</conditioncode1>
      <conditioncode2>Head and neck</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Hypofractionated radiotherapy, 30 Gray in 5 fractions, optional 6th 5 Gray fraction.</interventions>
    <comparator>None - single arm study</comparator>
    <control>Uncontrolled</control>
    <interventioncode>Treatment: Other</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Quality of life</outcome>
      <timepoint>Assessed at baseline, 1, 2, 3, 6, 9 and 12 months post treatment.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Analgesic requirements and radiation toxicity</outcome>
      <timepoint>Assessed at baseline, weekly during treatment and at 1, 2, 3, 6, 9 and 12 months post treatment.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Local control</outcome>
      <timepoint>Assessed at 1, 2, 3, 6, 9 &amp; 12 months post treatment. </timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Survival</outcome>
      <timepoint>Assessed up to 12 months post treatment.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Biopsy proven squamus cell carcinoma (primary or regional nodal disease) of the head and neck, ineligible for curative treatment, performance status 0-2, life expectanccy of at least 4 months, written informed consent.</inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
    <inclusiveminagetype>Not stated</inclusiveminagetype>
