    <scientifictitle>The effectiveness of dietry intervention in the management of women with faecal incontinence.</scientifictitle>
    <utrn />
    <trialacronym />
  </trial_identification>
  <conditions>
    <healthcondition>Anal incontinence/ defecation dysfunction</healthcondition>
    <conditioncode>
      <conditioncode1>Diet and nutrition</conditioncode1>
      <conditioncode2>Normal oral and gastrointestinal development and function</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Cross over dietary intervention study looking at the effect of modified fibre on bowel function over a period of 12 weeks.</interventions>
    <comparator />
    <control>Active</control>
    <interventioncode>None</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Cleveland Clinic continence scores, with improvement of &gt; 50%</outcome>
      <timepoint>Measured at completion of 12 week dietary intervention.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>VAS of patient satisfaction, SF 36, Anorectal Symptom Questionaire, QOL Faecal Incontinence, at completion of 12 week dietary intervention.</outcome>
      <timepoint />
    </secondaryOutcome>
  </outcomes>
  <eligibility>
