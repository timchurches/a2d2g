  <trial_identification>
    <studytitle>The effect of iodine supplementation on cognition and well-being in mildly deficient young adults</studytitle>
    <scientifictitle>A randomised controlled trial to determine the effect of iodine supplementation on cognition and well-being in mildly deficient young adults</scientifictitle>
    <utrn>U1111-1115-5764</utrn>
    <trialacronym>THINK2</trialacronym>
    <secondaryid>Nil</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>cognition, well-being and iodine status</healthcondition>
    <conditioncode>
      <conditioncode1>Diet and nutrition</conditioncode1>
      <conditioncode2>Other diet and nutrition disorders</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Mental Health</conditioncode1>
      <conditioncode2>Studies of normal psychology, cognitive function and behaviour</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>150 micrograms of iodine/day orally for 32 weeks</interventions>
    <comparator>placebo (eg sugar pill identical to intervention exposure)/day orally for 36 weeks</comparator>
    <control>Placebo</control>
    <interventioncode>Treatment: Other</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Cognition as measured by subtests of the Weschler Adult Intelligence Scales.</outcome>
      <timepoint>Baseline and 32 weeks</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Well-being as measured by Short-Form 36 (SF-36).</outcome>
      <timepoint>Baseline and 32 weeks</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Iodine status as measured by urinary iodine concentration, and thyroglobulin</outcome>
      <timepoint>Baseline and 32 weeks</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>18-30 years
healthy
free of thyroid disease
not pregnant
not taking iodine containing supplements
mildly iodine deficient</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
