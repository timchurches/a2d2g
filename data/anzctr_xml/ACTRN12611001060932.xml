  <actrnumber>ACTRN12611001060932</actrnumber>
  <trial_identification>
    <studytitle>An observational pilot study of 16 weeks of anti inflammatory doses of fish oils in patients with symptomatic osteoarthritis of the hands and the effect on pain levels</studytitle>
    <scientifictitle>An observational pilot study of 16 weeks of anti inflammatory doses of fish oils in patients with symptomatic osteoarthritis of the hands</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>nil known</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>osteoarthritis</healthcondition>
    <conditioncode>
      <conditioncode1>Musculoskeletal</conditioncode1>
      <conditioncode2>Osteoarthritis</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>high dose fish oil, 5 capsules daily for 16 weeks for patients precribed 2.7 - 3.6 grams of combination DHA and EPA by their physicians</interventions>
    <comparator>nil</comparator>
    <control>Uncontrolled</control>
    <interventioncode>Not applicable</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Reduction in pain VAS</outcome>
      <timepoint>16 weeks</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Musculosketelal Ultrasound of hand joints and the degree of reduction in measures of inflammation</outcome>
      <timepoint>16 weeks</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>symptomatic osteoarthritis hands with stable doses of analgesics who meet the ACR criteria for osteoarthritis of the hands who are presribed high dose fish oil by their treating doctors</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>No limit</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>prescence of an inflammatory arthritis and subjects unwilling to give informed consent</exclusivecriteria>
  </eligibility>
