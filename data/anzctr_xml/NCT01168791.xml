  <conditions>
    <healthcondition>Soft Tissue Sarcoma</healthcondition>
    <conditioncode>
      <conditioncode1>Cancer</conditioncode1>
      <conditioncode2>Sarcoma (also see 'Bone') - soft tissue</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Cancer</conditioncode1>
