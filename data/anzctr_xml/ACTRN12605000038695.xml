    <studytitle>Supermarket Healthy Options Project (SHOP) - pilot phase</studytitle>
    <scientifictitle>Feasibility study of the effect of price discounts and nutrition education on food purchases in supermarkets</scientifictitle>
    <utrn />
    <trialacronym>SHOP</trialacronym>
    <secondaryid>SHOP</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Not applicable
</healthcondition>
    <healthcondition>General population</healthcondition>
    <conditioncode>
      <conditioncode1>Other</conditioncode1>
      <conditioncode2>Research that is not of generic health relevance and not applicable to specific health categories listed above</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>1. Price dscounts on fruit and vegetables (fresh, canned, frozen)
2. Culturally appropriate nutrition education promoting purchase of fruit and vegetables
3. Combination of 1 and 2</interventions>
    <comparator>Control (no intervention)</comparator>
    <control>Active</control>
    <interventioncode>Behaviour</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Purchase of fruit and vegetables over a 12-week period</outcome>
      <timepoint>Study end</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Evaluation of supermarket purchases as a proportion of all food purchases.</outcome>
      <timepoint>Study end</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>1. Main household food shopper 2. Shops at participating supermarket 3. Spends at least $200 per month at supermarket 4. Active user of Shop 'N Go system 5. 12 months of retrospective electronic shopping data available 6. Buys at least 2/3 fruit and vegetables at supermarkets.</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>Not stated</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>1. Participant in study focus groups 2. Do not intend to use Shop 'N Go system at participating supermarket during study period.</exclusivecriteria>
  </eligibility>
