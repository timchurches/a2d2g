<ANZCTR_Trial requestNumber="82035">
  <stage>Registered</stage>
  <submitdate>21/05/2007</submitdate>
  <approvaldate>22/05/2007</approvaldate>
  <actrnumber>ACTRN12607000275460</actrnumber>
  <trial_identification>
    <studytitle>A Phase I, Single Ascending Dose, Safety and Pharmacokinetic Study of PN0621 in Healthy Volunteers</studytitle>
    <scientifictitle>A Phase I, Single Ascending Dose, Safety and Pharmacokinetic Study of PN0621 in Healthy Volunteers</scientifictitle>
    <utrn />
    <trialacronym>PN0621/100</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Inflammatory dieseases, Healthy Volunteers</healthcondition>
    <conditioncode>
      <conditioncode1>Inflammatory and Immune System</conditioncode1>
      <conditioncode2>Other inflammatory or immune system disorders</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>PN0621 'IV-Intravenous' administation starting dose 5mg then increasing in new healthy volunteers for each dose through 10mg,20mg,40mg, 80mg and 160mg until a dose limiting toxicity is reached 'SC-subcutaneous' administration starting dose 20mg then increasing in new healthy volunteers for each dose through 40mg, 80mg, 160mg and 320mg until a dose limiting toxicity is reached.</interventions>
    <comparator />
    <control>Uncontrolled</control>
    <interventioncode>Treatment: drugs</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Safety at baseline for IV administration</outcome>
      <timepoint>At baseline and then at 16 days.</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Safety for SC administration</outcome>
      <timepoint>At baseline and then at 20 days.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Determination of pharmacokinetic parameters</outcome>
      <timepoint>(Measured every other day) over 16 days for IV administration and 20 days for SC administration.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>BMI - Body mass Index' 19 to 29, nromal or clinically acceptable 'ECG - Electrocardiogram'</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>40</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Donation or loss of more than 400ml blood within 3 months of study, history of infection, screen positive to Tuberculosis, Hepatitis C, Hepatisis B or Human immunodeficiency virus (HIV)</exclusivecriteria>
  </eligibility>
