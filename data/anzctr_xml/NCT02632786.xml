  <outcomes>
    <primaryOutcome>
      <outcome>Cardiac best response as measured by NT-proBNP</outcome>
      <timepoint>Baseline to 12 months</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Change in Short Form-36 (SF-36) Questionnaire</outcome>
