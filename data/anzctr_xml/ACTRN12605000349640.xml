  <stage>Registered</stage>
  <submitdate>8/09/2005</submitdate>
  <approvaldate>9/09/2005</approvaldate>
  <actrnumber>ACTRN12605000349640</actrnumber>
  <trial_identification>
    <studytitle>The effect of manual chest physiotherapy, positive expiratory pressure (PEP), and oscillating PEP on mucociliary clearance in subjects with CF</studytitle>
    <scientifictitle>The effect of manual chest physiotherapy, positive expiratory pressure (PEP), and oscillating PEP on mucociliary clearance in subjects with CF</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>X03-0052</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Cystic fibrosis</healthcondition>
    <conditioncode>
      <conditioncode1>Human genetics and inherited disorders</conditioncode1>
      <conditioncode2>Cystic fibrosis</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Physical Medicine / Rehabilitation</conditioncode1>
      <conditioncode2>Physiotherapy</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Postural DrainagePositive Expiratory Pressure (PEP) PhysiotherapyOscillating PEP (Flutter).</interventions>
    <comparator>Also, cough control, but this intervention by necessity is a post comparison, not randomised like the first four interventions.Each intervention is for 20 minutes.</comparator>
    <control>Placebo</control>
    <interventioncode>None</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Whole right lung mucociliary clearance</outcome>
      <timepoint>Immediately after treatment.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Regional (central, peripheral) clearance</outcome>
      <timepoint>Immediately after treatment, follow-up clearance during the subsequent 90 minutes.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Cough</outcome>
      <timepoint>During and for 90 minutes after treatment.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Sputum weight </outcome>
      <timepoint>During treatment, perceived effect immediately after followup.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Cystic fibrosis, stable clinical condition (FEV1 within 10% of best for the previous 6 months).</inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
    <inclusiveminagetype>Not stated</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
