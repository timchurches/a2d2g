<ANZCTR_Trial requestNumber="343020">
  <stage>Registered</stage>
  <submitdate>3/06/2011</submitdate>
  <approvaldate>20/06/2011</approvaldate>
  <actrnumber>ACTRN12611000623998</actrnumber>
  <trial_identification>
    <studytitle>Thermal Imaging of Stress Injuries in The Lower Limbs</studytitle>
    <scientifictitle>Objective clinical assessment of the efficacy of longwave infrared interrogation of osseous stress pathophysiology in the anteromedial pretibial tissues of Australian Army basic trainees</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>NIL</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Osseous stress pathology</healthcondition>
    <conditioncode>
      <conditioncode1>Musculoskeletal</conditioncode1>
      <conditioncode2>Other muscular and skeletal disorders</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Passive, non-invasive, non-contact, thermal infrared thermometry of anteromedial aspect of lower leg. supervised by a member of the investigation team; one image acquired per participant, every 4 weeks, for 12 weeks. 1 image acquired on day 0, thus 4 images in total per participant.</interventions>
    <comparator>None- Not applicable</comparator>
    <control>Uncontrolled</control>
    <interventioncode>Not applicable</interventioncode>
    <interventioncode>Early detection / Screening</interventioncode>
    <interventioncode>Treatment: drugs</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>correlation of longwave infrared imaging aberration to MRI aberration</outcome>
      <timepoint>1 to less than 6 months after image acquisition</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Correlation of longwave infrared imaging aberration to nuclear scintigraphic aberration</outcome>
      <timepoint>6 months after image acquisition</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Fit for Australian Regular Army basic training</inclusivecriteria>
    <inclusiveminage>17</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>35</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>Yes</healthyvolunteer>
    <exclusivecriteria>Contraindicative medical history, e.g. previous lower-limb fracture, superficial varicosity within region of interest, dermal abnormality</exclusivecriteria>
  </eligibility>
