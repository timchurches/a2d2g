<ANZCTR_Trial requestNumber="821">
  <stage>Registered</stage>
  <submitdate>7/10/2005</submitdate>
  <approvaldate>2/11/2005</approvaldate>
  <actrnumber>ACTRN12605000709640</actrnumber>
  <trial_identification>
    <studytitle>A randomised trial of a multidisciplinary teamcare approach involving obstetric, dietary and clinical psychological input in obese pregnant women to reduce the incidence of gestational diabetes</studytitle>
    <scientifictitle>A multidisciplinary teamcare approach involving obstetric, dietary and clinical psychological input in obese pregnant women to reduce the incidence of gestational diabetes</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>Perinatal Trials Registry: PTR543</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Obese pregnant women</healthcondition>
    <conditioncode>
      <conditioncode1>Diet and nutrition</conditioncode1>
      <conditioncode2>Obesity</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Reproductive Health and Childbirth</conditioncode1>
      <conditioncode2>Normal pregnancy</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Multidisciplinary antenatal care involving obstetric, dietary, clinical psychology input compared to current care, applied from the first antenatal visit until delivery of the baby applied at antenatal visits the number of which will be determined by medical indications for each woman</interventions>
    <comparator>Normal antenatal care as defined by the three centres consensus on antenatal care (Victoria) which defines the type and number of antenatal visits</comparator>
    <control>Active</control>
    <interventioncode>Prevention</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Reduction in gestational diabetes as diagnosed by glucose tolerance test</outcome>
      <timepoint>At 28 weeks gestation and measured at delivery</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>1. Pooled adverse events in pregnancy</outcome>
      <timepoint>Measured at delivery.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>2. Pooled adverse events in fetus or newborn </outcome>
      <timepoint>Measured 6 weeks postpartum.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>3. Initiation of breastfeeding and breastfeeding at hospital discharge</outcome>
      <timepoint>Measured on Day 5 postpartum.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>BMI at booking visit greater than 25, speak english, no known fetal anomaly, able to attend for antenatal care, present less than 20 weeks gestation.</inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
    <inclusiveminagetype>Not stated</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>Not stated</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
