      <outcome>Sustained virologic response (SVR)</outcome>
      <timepoint>Week 72</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Rapid virologic response</outcome>
      <timepoint>Week 4</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Early virologic response</outcome>
      <timepoint>Week 12</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Undetectable HCV RNA</outcome>
