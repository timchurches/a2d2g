  <outcomes>
    <primaryOutcome>
      <outcome>Change in body weight and proportion of subjects with 5% weight loss</outcome>
      <timepoint>1 year</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Waist circumference, blood lipids and glucose, prevalence of metabolic syndrome, patient reported outcomes, population PK</outcome>
      <timepoint>1-2 years</timepoint>
    </secondaryOutcome>
  </outcomes>
