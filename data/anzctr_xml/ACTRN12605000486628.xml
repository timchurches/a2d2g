  <actrnumber>ACTRN12605000486628</actrnumber>
  <trial_identification>
    <studytitle>Predicting improvement in functional outcomes with aripiprazole</studytitle>
    <scientifictitle>Open label naturalistic six month study of aripiprazole in schizophrenia measuring functional outcomes and positive, negative and cognitive symptoms</scientifictitle>
    <utrn />
    <trialacronym>Double A Study</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Schizophrenia and related disorders</healthcondition>
    <conditioncode>
      <conditioncode1>Mental Health</conditioncode1>
      <conditioncode2>Schizophrenia</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Open label naturalistic flexible dosing study with aripiprazole for six months.</interventions>
    <comparator />
    <control>Uncontrolled</control>
    <interventioncode>Treatment: drugs</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>The Assessment of Motor and Process Skills (AMPS: Pan and Fisher, 1994)</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The World Health Organization Disability Assessment Schedule 2 (WHO-DAS 2: http://www.who.int/icidh/whodas)</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The PANSS (Positive and Negative Symptom Scale)</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The RBANS (Repeatable Battery for the Assessment of Neuropsychological Status) list learning task</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The Penn Face Recognition Test (PFRT)</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The Penn Emotional Acuity Test (PEAT)</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The RBANS verbal fluency (semantic)</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The Wisconsin Card Sort Test (WCST)</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>The Go no-go test</outcome>
      <timepoint>Measured at baseline, 12 weeks and 6 months</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Simpson-Angus Extrapyramidal Rating Scale</outcome>
      <timepoint>Measured at baseline, 12 weeks and six months.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Calgary Depression Rating Scale</outcome>
      <timepoint>Measured at baseline, 12 weeks and six months.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>DSM-IV diagnosis of schizophrenia or related disorder.</inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
    <inclusiveminagetype>Not stated</inclusiveminagetype>
