  <trial_identification>
    <studytitle>Effects of a preventive back care program on students level of knowledge</studytitle>
    <scientifictitle>Effects of a preventive back care program on school students' level of knowledge</scientifictitle>
    <utrn />
    <trialacronym />
    <secondaryid>nil</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>low back pain</healthcondition>
    <healthcondition>postural deviation</healthcondition>
    <conditioncode>
      <conditioncode1>Musculoskeletal</conditioncode1>
      <conditioncode2>Other muscular and skeletal disorders</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Public Health</conditioncode1>
      <conditioncode2>Health promotion/education</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>back school children program including lessons on back anatomy and physiology, proper sitting posture, lifting techniques. All lessons were given at school, during the class period and lasted 50 minutes each one. The program was comprised by three sessions aplied by two physiotherapists with one week apart.</interventions>
    <comparator>no control group</comparator>
    <control>Uncontrolled</control>
    <interventioncode>Prevention</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>level of theoretical and practical knowledge about back care assessed by questionnaires and practical test simulating real situations of manual material handling</outcome>
      <timepoint>imediately after the third and final session intervention</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>no sencondary outcome</outcome>
      <timepoint>no</timepoint>
    </secondaryOutcome>
  </outcomes>
