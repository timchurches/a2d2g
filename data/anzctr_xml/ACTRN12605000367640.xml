  <trial_identification>
    <studytitle>RCT of laser acupuncture in acute low back pain.</studytitle>
    <scientifictitle>Double blind randomised controlled trial of laser acupuncture in acute and subacute low back pain as measured by Oswestry disability index.</scientifictitle>
    <utrn />
    <trialacronym>LoBACu</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Acute and subacute low back pain</healthcondition>
    <conditioncode>
      <conditioncode1>Alternative and complementary medicine</conditioncode1>
      <conditioncode2>Pain management</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>8 weeks laser acupuncture </interventions>
    <comparator>8 weeks sham laser acupuncture</comparator>
    <control>Placebo</control>
    <interventioncode>Prevention</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Oswestry Disability Index</outcome>
      <timepoint>Measured weekly for 8 weeks, then at follow up (3, 6, 9, 12 months)</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>K10</outcome>
      <timepoint>Measured at commencement and follow up (3,6,9,12 months).</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>VAS</outcome>
      <timepoint>Measured weekly for 8 weeks, then at follow up (3,6,9,12 months).</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Medication</outcome>
      <timepoint>Measured weekly for 8 weeks, then at follow up (3,6,9,12 months).</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Health Utilization</outcome>
      <timepoint>Measured weekly for 8 weeks, then at follow up (3,6,9,12 months).</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>2-8 wks of back pain.</inclusivecriteria>
    <inclusiveminage>20</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>60</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Chronic LBP, significant medical conditions.</exclusivecriteria>
  </eligibility>
  <trial_design>
    <studytype>Interventional</studytype>
    <purpose>Prevention</purpose>
    <allocation>Randomised controlled trial</allocation>
    <concealment>yes - independent allocation - wired and sealed into laser machine with no access and no bypass switch</concealment>
    <sequence>random numbers generated in Excel, stratification by GP and block randomisation.</sequence>
    <masking>Blinded (masking used)</masking>
