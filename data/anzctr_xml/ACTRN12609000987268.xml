<ANZCTR_Trial requestNumber="320832">
  <stage>Registered</stage>
  <submitdate>13/11/2009</submitdate>
  <approvaldate>17/11/2009</approvaldate>
  <actrnumber>ACTRN12609000987268</actrnumber>
  <trial_identification>
    <studytitle>The effect of intra-dialytic exercise training on pro-inflammatory cytokines, endothelial function and functional capacity in chronic kidney disease patients.</studytitle>
    <scientifictitle>The effect of intra-dialytic exercise training versus usual care on functional capacity, systematic inflammation, quality of life, blood vessel function and biochemistry in patients with chronic kidney disease.</scientifictitle>
    <utrn>U1111-1112-5020</utrn>
    <trialacronym>None</trialacronym>
    <secondaryid>None</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Chronic Kidney Disease</healthcondition>
    <conditioncode>
      <conditioncode1>Renal and Urogenital</conditioncode1>
      <conditioncode2>Kidney disease</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Exercise training (cycling) for 45 minutes, 3 times per week for 3 months at about 65% maximum.</interventions>
    <comparator>Usual Care is dialysis 3 sessions per week for approxiamtely 4 hours per session and nephrology consultations as required.</comparator>
    <control>Active</control>
    <interventioncode>Rehabilitation</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Peak Volume of oxygen (VO2) measured by incremental, maximal exercise test conducted on a cycle ergometer.</outcome>
      <timepoint>0, 3, 6 months following randomization</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Quality of life. This measurement will be made using the Kidney Disease Quality of Life (KDQOL) questionnaire.</outcome>
      <timepoint>0, 3, 6 months following randomization</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Systemic Inflammation interleukin -1(IL-1), interleukin-6 (IL-6), interleukin-10 (IL-10), tumour necrosis factor alpha (TNF-alpha) and TNF-alpha soluble receptors I and II will be measured by immunoassay.</outcome>
      <timepoint>0, 3, 6 months following randomization</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Blood vessel function by Doppler ultrasound</outcome>
      <timepoint>0, 3, 6 months following randomization</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Nil</outcome>
      <timepoint>Nil</timepoint>
    </secondaryOutcome>
  </outcomes>
