  <trial_identification>
    <studytitle>Neonatal Core body temperature investigation</studytitle>
    <scientifictitle>Neonatal core body temperature measurement: a pilot study</scientifictitle>
    <utrn>U1111-1119-3435</utrn>
    <trialacronym />
    <secondaryid>Nil</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>body temperature</healthcondition>
    <conditioncode>
      <conditioncode1>Reproductive Health and Childbirth</conditioncode1>
      <conditioncode2>Childbirth and postnatal care</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Observational pilot study of body temperature on critically ill neonates for up to 24 hours. Rectal, Oesophageal, non-invasive thoracic (middlle of the back in contact with the mattress), non-invasive head (middlle of the head in contact with the mattress) and exposed abdominal skin temperatures are recorded on an a battery operated datalogger every 10 seconds.</interventions>
    <comparator>not applicable</comparator>
    <control>Uncontrolled</control>
    <interventioncode>Not applicable</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>body temperature from 5 sites is recorded every 10 seconds on a battery operated datalogger</outcome>
      <timepoint>for up to 24 hours</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Nil</outcome>
      <timepoint>Nil</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
