  <actrnumber>ACTRN12608000106336</actrnumber>
  <trial_identification>
    <studytitle>VENTRASSIST速 exercise study in heart failure patients</studytitle>
    <scientifictitle>Investigation into the effect of pump speed on exercise capacity in VENTRASSIST left ventricular assist patients</scientifictitle>
    <utrn />
    <trialacronym />
  </trial_identification>
  <conditions>
    <healthcondition>Exercise capacity</healthcondition>
    <conditioncode>
      <conditioncode1>Cardiovascular</conditioncode1>
      <conditioncode2>Other cardiovascular diseases</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Exercise capacity, relationship between pump speed and oxygen consumption (VO2 max) measured at three pump speeds over a three day period, VENTRASSIST速, left ventricular assist device</interventions>
    <comparator>N/A</comparator>
    <control>Uncontrolled</control>
    <interventioncode>Not applicable</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Relationship between pump speed and oxygen consumption, measured as peak exercise oxygen consumption (VO2) in ml/kg/min at each speed setting</outcome>
      <timepoint>Day 1,2 and 3 of exercise</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Investigation of patient's anaerobic threshold at different pump speeds</outcome>
      <timepoint>Day 1, 2 and 3 of exercise</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Examining effects of different pump speeds during maximal exercise on patient's cardiopulmonary measurements</outcome>
      <timepoint>Dat 1, 2 and 3 of exercise</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>VentrAssist left ventricular assist device implanted, signed informed consent, age greater than or equal to 18 years, clinically stable as assessed by cardiologist, ambulatory</inclusivecriteria>
    <inclusiveminage>18</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>No limit</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria>Active infection, exercise induced myocardial ischaemia or significant arrhythmias, inability to achieve a significant increase in left ventricular assist device flow without risk of suction as determined by clinical judgment, VENTRASSIST速 left ventricular assist device implant duration less than 2 months</exclusivecriteria>
  </eligibility>
