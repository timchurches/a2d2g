  <conditions>
    <healthcondition>sleep disordered breathing (sleep apnoea)</healthcondition>
    <healthcondition>Acute stroke</healthcondition>
    <conditioncode>
      <conditioncode1>Respiratory</conditioncode1>
      <conditioncode2>Sleep apnoea</conditioncode2>
