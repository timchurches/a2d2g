  <trial_identification>
    <studytitle>Long-Term Safety and Efficacy of rFIXFc in the Prevention and Treatment of Bleeding Episodes in Previously Treated Participants With Hemophilia B</studytitle>
    <scientifictitle>An Open-Label, Multicenter, Evaluation of the Long-Term Safety and Efficacy of Recombinant Human Coagulation Factor IX Fusion Protein (rFIXFc) in the Prevention and Treatment of Bleeding Episodes in Previously Treated Subjects With Hemophilia B</scientifictitle>
    <utrn />
    <trialacronym>B-YOND</trialacronym>
    <secondaryid>2011-003075-11</secondaryid>
    <secondaryid>9HB01EXT</secondaryid>
  </trial_identification>
  <conditions>
    <healthcondition>Severe Hemophilia B</healthcondition>
    <conditioncode>
      <conditioncode1>Blood </conditioncode1>
      <conditioncode2>Clotting disorders</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Human Genetics and Inherited Disorders</conditioncode1>
