  <trial_identification>
    <studytitle>A consumer-adoption study: Exercise training for the healthy elderly</studytitle>
    <scientifictitle>A consumer-adoption study: A randomized controlled trial of exercise training for older adults in an Australian retirement community setting to improve physical function and health-related quality of life.</scientifictitle>
    <utrn />
    <trialacronym>BEST :  Balance, Endurance, and Strength Training</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Healthy elderly population</healthcondition>
    <conditioncode>
      <conditioncode1>Public Health</conditioncode1>
      <conditioncode2>Health promotion/education</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>A 10 week multi-modal , supervised exercise program (balance, endurance, and high intensity progressive resistance training).</interventions>
    <comparator />
    <control>Active</control>
    <interventioncode>None</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Strength</outcome>
      <timepoint>Measured at baseline and after either a 10 week intervention or no intervention control.</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Balance</outcome>
      <timepoint>Measured at baseline and after either a 10 week intervention or no intervention control.</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Performance-based function</outcome>
      <timepoint>Measured at baseline and after either a 10 week intervention or no intervention control.</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Health-related quality of life. </outcome>
      <timepoint>Measured at baseline and after either a 10 week intervention or no intervention control.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Depressive symptoms.</outcome>
      <timepoint>Measured at baseline and after either a 10 week intervention or no intervention control.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Self-efficacy.</outcome>
      <timepoint>Measured at baseline and after either a 10 week intervention or no intervention control.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Behavioural outcomes.</outcome>
      <timepoint>Measured at baseline and after either a 10 week intervention or no intervention control.</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Customer intentions (intention to stay, intention to recommend).</outcome>
      <timepoint>Measured at baseline and after either a 10 week intervention or no intervention control.</timepoint>
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria> Residents of retirement village.</inclusivecriteria>
    <inclusiveminage>60</inclusiveminage>
    <inclusiveminagetype>Years</inclusiveminagetype>
    <inclusivemaxage>100</inclusivemaxage>
    <inclusivemaxagetype>Years</inclusivemaxagetype>
