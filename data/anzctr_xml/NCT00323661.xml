      <timepoint>24 months</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Quality of life assessed by the SF-08</outcome>
      <timepoint>12 months and 24 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>occurrence of atrial fibrillation</outcome>
      <timepoint>12 months and 24 months</timepoint>
