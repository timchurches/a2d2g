    <secondaryOutcome>
      <outcome>occurrence of serious adverse events</outcome>
      <timepoint>12 months and 24 months</timepoint>
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>self-assessment of general well-being, mental and physical performance assessed by Visual Analogue Scales</outcome>
      <timepoint>12 months and 24 months</timepoint>
    </secondaryOutcome>
