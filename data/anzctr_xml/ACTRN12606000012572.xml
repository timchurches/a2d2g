    <utrn />
    <trialacronym>TS-KNOBa</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Obstructive Sleep Apnea</healthcondition>
    <conditioncode>
      <conditioncode1>Respiratory</conditioncode1>
      <conditioncode2>Sleep apnoea</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Participants will be randomised to receive CPAP with 
NO humidity OR
Conventional humidity OR 
Humidity via a Novel humidifier
for 12 weeks. 

Duration:  12 weeks 
Patient compliance and satisfaction with their treatment will be assessed.</interventions>
    <comparator>No humidity</comparator>
    <control>Active</control>
    <interventioncode>None</interventioncode>
  </interventions>
