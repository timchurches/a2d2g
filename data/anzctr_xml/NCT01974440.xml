  <conditions>
    <healthcondition>Lymphoma</healthcondition>
    <conditioncode>
      <conditioncode1>Cancer</conditioncode1>
      <conditioncode2>Lymphoma (non Hodgkin's lymphoma) - High grade lymphoma</conditioncode2>
    </conditioncode>
    <conditioncode>
