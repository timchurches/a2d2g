<ANZCTR_Trial requestNumber="130">
  <stage>Registered</stage>
  <submitdate>5/08/2005</submitdate>
  <approvaldate>9/08/2005</approvaldate>
  <actrnumber>ACTRN12605000110684</actrnumber>
  <trial_identification>
    <studytitle>Worksite intervention effects on psychological and physical health: A randomised controlled trial</studytitle>
    <scientifictitle>A randomised controlled trial study to evaluate the effects of exercise plus health education on psychological and physical health outcomes in healthy sedentary employees from a single worksite</scientifictitle>
    <utrn />
    <trialacronym />
  </trial_identification>
  <conditions>
    <healthcondition>Depressive, anxious, and stress symptoms, </healthcondition>
    <healthcondition>Self-rated sleep quality</healthcondition>
    <healthcondition>Quality-of-life outcomes,</healthcondition>
    <healthcondition>Physical health</healthcondition>
    <conditioncode>
      <conditioncode1>Mental Health</conditioncode1>
      <conditioncode2>Depression</conditioncode2>
    </conditioncode>
    <conditioncode>
      <conditioncode1>Mental Health</conditioncode1>
      <conditioncode2>Studies of normal psychology, cognitive function and behaviour</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>A 24 week randomised controlled trial of exercise plus dietary behavioral intervention inlcuding sleep hygiene.</interventions>
    <comparator>Waitlist controls</comparator>
    <control>Historical</control>
    <interventioncode>Treatment: Other</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Depression</outcome>
      <timepoint>Measured at pre-test (baseline) and post-test (24 weeks)</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Anxiety</outcome>
      <timepoint>Measured at pre-test (baseline) and post-test (24 weeks)</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Stress Scales (DASS)</outcome>
      <timepoint>Measured at pre-test (baseline) and post-test (24 weeks)</timepoint>
    </primaryOutcome>
    <primaryOutcome>
      <outcome>Quality-of-Life (SF-36 Health Status Survey)</outcome>
      <timepoint>Measured at pre-test (baseline) and post-test (24 weeks)</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Waist circumference</outcome>
      <timepoint />
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Body mass</outcome>
      <timepoint />
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Body mass index</outcome>
      <timepoint />
    </secondaryOutcome>
    <secondaryOutcome>
      <outcome>Predicted maximal aerobic capacity (predicted VO2max), (note that the number 2 should be subscripted).</outcome>
      <timepoint />
    </secondaryOutcome>
  </outcomes>
  <eligibility>
    <inclusivecriteria>Healthy sedentary employees, able to produce a medical clearance to exercise, and willingness to be randomised to treatment or waitlist groups. </inclusivecriteria>
    <inclusiveminage>0</inclusiveminage>
    <inclusiveminagetype>Not stated</inclusiveminagetype>
    <inclusivemaxage>0</inclusivemaxage>
    <inclusivemaxagetype>Not stated</inclusivemaxagetype>
    <inclusivegender>Both males and females</inclusivegender>
    <healthyvolunteer>No</healthyvolunteer>
    <exclusivecriteria> Inability to commit to intervention, and medical conditions severly limiting regular exercise.</exclusivecriteria>
  </eligibility>
