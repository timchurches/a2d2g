  <outcomes>
    <primaryOutcome>
      <outcome>Proportion of Subjects With a Serum Urate (sUA) Level That is &lt; 6.0 mg/dL</outcome>
      <timepoint>Month 1</timepoint>
    </primaryOutcome>
