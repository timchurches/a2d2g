  <trial_identification>
    <studytitle>Transient amnesia assessment and management in an emergency department</studytitle>
    <scientifictitle>Do patients with acute memory disturbance following straining have a higher proportion of jugular venous backflow than those that present without straining?</scientifictitle>
    <utrn />
    <trialacronym>TAMED</trialacronym>
  </trial_identification>
  <conditions>
    <healthcondition>Transient Amnesia</healthcondition>
    <conditioncode>
      <conditioncode1>Neurological</conditioncode1>
      <conditioncode2>Other neurological disorders</conditioncode2>
    </conditioncode>
  </conditions>
  <interventions>
    <interventions>Ultrasound scan of bilateral internal jugular veins - once only scan lasting for approx 10 minutes.</interventions>
    <comparator>Nil</comparator>
    <control>Uncontrolled</control>
    <interventioncode>Not applicable</interventioncode>
  </interventions>
  <outcomes>
    <primaryOutcome>
      <outcome>Proportion of participants with internal jugular reflux during Valsalva manouvre</outcome>
      <timepoint>Feb 2008 to Feb 2010</timepoint>
    </primaryOutcome>
    <secondaryOutcome>
      <outcome>Proportion of participants with recurrence, acute neurological event, cardiovascular event, death</outcome>
      <timepoint>Feb 2010</timepoint>
    </secondaryOutcome>
  </outcomes>
